   if key == 'w' or key == 'W':
        rect_y -= speedY
    elif key == 's' or key == 'S':
        rect_y += speedY
    elif key == 'a' or key == 'A':
        rect_x -= speedX
    elif key == 'd' or key == 'D':
        rect_x += speedX
    elif key == 'q' or key == 'Q':
        rect_y =  rect_y
        rect_x = rect_x
